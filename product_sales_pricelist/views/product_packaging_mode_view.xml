<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data>
		<record id="product_packaging_tree_view" model="ir.ui.view">
			<field name="name">product_package_mode_tree_view</field>
			<field name="model">product.packaging.mode</field>
			<field name="arch" type="xml">
				<tree>
                    <field name="packaging_mode"/>
				</tree>
			</field>
		</record>

		<record id="product_packaging_form_view" model="ir.ui.view">
			<field name="name">product_package_form_view</field>
			<field name="model">product.packaging.mode</field>
			<field name="type">form</field>
			<field name="arch" type="xml">
				<form string="Product Packaging Mode">
					<sheet>
						<group>
							<field name="packaging_mode"/>
						</group>
	                </sheet>
				</form>
			</field>
		</record>

    </data>
</odoo>