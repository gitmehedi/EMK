<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data>

        <record id="inherit_partner_acc" model="ir.ui.view">
            <field name="name">res_partner_inh</field>
            <field name="model">res.partner</field>
            <field name="inherit_id" ref="account.view_partner_property_form"/>
            <field name="arch" type="xml">

                <xpath expr="//group[@name='accounting_entries']" position="replace">
                    <group string="Accounting Entries" name="accounting_entries"
                           groups="gbs_application_group.group_sales_customer_manager,account.group_account_manager,account.group_account_user">
                        <field name="property_account_receivable_id"/>
                        <field name="property_account_payable_id"/>
                    </group>
                </xpath>

            </field>
        </record>

    </data>
</odoo>