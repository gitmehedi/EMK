<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data noupdate="1">

        <record id="commission_journal_entry_sequence" model="ir.sequence">
            <field name="name">Commission Journal Entry Sequence</field>
            <field name="code">commission.account.move.seq</field>
            <field name="prefix">COM/%(year)s/</field>
            <field name="number_next">1</field>
            <field name="number_increment">1</field>
            <field name="padding">7</field>
        </record>

        <record id="refund_journal_entry_sequence" model="ir.sequence">
            <field name="name">Refund Journal Entry Sequence</field>
            <field name="code">refund.account.move.seq</field>
            <field name="prefix">REF/%(year)s/</field>
            <field name="number_next">1</field>
            <field name="number_increment">1</field>
            <field name="padding">7</field>
        </record>

    </data>
</odoo>