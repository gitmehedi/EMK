<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data>

        <record id="inherited_company_form_view_cash_suspense" model="ir.ui.view">
            <field name="name">inherit_company_form_v</field>
            <field name="model">res.company</field>
            <field name="inherit_id" ref="gbs_account.inherited_company_form_view"/>
            <field name="arch" type="xml">
                <xpath expr="//group[@name='general']" position="before">
                    <group>
                        <field name="account_receive_clearing_acc" options="{'no_quick_create':True,'no_create_edit':True,'no_open':True}"/>
                        <field name="journal_id" options="{'no_quick_create':True,'no_create_edit':True,'no_open':True}"/>
                    </group>
                </xpath>

            </field>
        </record>

    </data>
</odoo>