<?xml version="1.0"?>
<odoo>

    <record id="suspense_cash_account_form" model="ir.ui.view">
        <field name="name">conf.credit.acc.form</field>
        <field name="model">conf.credit.acc</field>
        <field name="arch" type="xml">
            <form string="Suspense Cash Acc">
                <sheet>
                    <group>
                        <field name="cash_suspense_account"/>
                    </group>

                </sheet>

            </form>
        </field>
    </record>

    <record id="suspense_cash_account_tree" model="ir.ui.view">
        <field name="name">conf.credit.acc.tree.view</field>
        <field name="model">conf.credit.acc</field>
        <field name="type">tree</field>
        <field name="arch" type="xml">
            <tree string="Suspense Cash Acc Tree">
                <field name="cash_suspense_account"/>
            </tree>
        </field>
    </record>

    <record model="ir.actions.act_window" id="action_suspense_cash_ac">
        <field name="name">Suspense Cash A/C</field>
        <field name="res_model">conf.credit.acc</field>
        <field name="view_mode">tree,form</field>
    </record>


       <menuitem
            action="action_suspense_cash_ac"
            id="menu_suspense_cash_ac"
            parent="account.account_account_menu"
            sequence="1"
    />

</odoo>