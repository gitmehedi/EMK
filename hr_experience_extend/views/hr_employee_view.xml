<?xml version="1.0" encoding="utf-8"?>
<odoo>

    <!-- inheritEmployee -->
    <record model="ir.ui.view" id="view_employee_form_inherit">
        <field name="name">hr.experience.employee.form</field>
        <field name="model">hr.employee</field>
        <field name="inherit_id" ref="hr_experience.view_employee_form_inherit"/>
        <field name="arch" type="xml">
            <xpath expr="//field[@name='academic_ids']/tree/field[@name='start_date']" position="before">
                <field name="passing_yr"/>
            </xpath>
            <xpath expr="//field[@name='academic_ids']/form/field[@name='start_date']" position="before">
                <field name="passing_yr"/>
            </xpath>
        </field>
    </record>

</odoo>
