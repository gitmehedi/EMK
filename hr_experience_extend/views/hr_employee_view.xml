<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data>
        <record model="ir.ui.view" id="view_employee_form_inherit_view">
            <field name="name">hr.experience.employee.form.view</field>
            <field name="model">hr.employee</field>
            <field name="inherit_id" ref="hr_experience.view_employee_form_inherit"/>
                <field name="arch" type="xml">
                    <xpath expr="//field/tree/field[@name='start_date']" position="before">
                        <field name="passing_yr"/>
                    </xpath>
                    <xpath expr="//field/form/group/field[@name='start_date']" position="before">
                        <field name="passing_yr"/>
                    </xpath>
                </field>
        </record>

    </data>


</odoo>
