<?xml version='1.0' encoding='UTF-8'?>
<odoo>
    <data noupdate="1">
        <record id="seq_asset_category" model="ir.sequence">
            <field name="name">Account Asset Category</field>
            <field name="code">account.asset.category</field>
            <field name="padding">4</field>
            <field name="number_increment" eval="1"/>
            <field name="company_id" eval="False"/>
        </record>

        <record id="seq_asset_depreciation_change_request" model="ir.sequence">
            <field name="name">Depreciation Change Request</field>
            <field name="code">asset.depreciation.change.request</field>
            <field name="active">TRUE</field>
            <field name="prefix">DMCR/CAT/</field>
            <field name="padding">2</field>
            <field name="number_increment">1</field>
        </record>
    </data>
</odoo>