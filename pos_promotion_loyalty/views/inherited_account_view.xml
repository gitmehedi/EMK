<?xml version="1.0" encoding="UTF-8"?>
<openerp>
	<data>
		<record  id="inherited_account_view" model="ir.ui.view">
			<field name="name">Inherited account view for Redemption point</field>
			<field name="model">account.journal</field>
			<field name="inherit_id" ref="account.view_account_journal_form"/>
			<field name="arch" type="xml">
				<xpath expr="//field[@name='type']" position="after">
					<field name="is_redemption" string="Redemption Point"/>
				</xpath>
			</field>
		</record>
	</data>
</openerp>
