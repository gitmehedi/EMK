<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data>

        <!-- holiday type -->
        <record id="view_date_range_type_tree_inherit" model="ir.ui.view">
            <field name="name">date.range.type.inherit.tree.view</field>
            <field name="model">date.range.type</field>
            <field name="inherit_id" ref="date_range.view_date_range_type_tree" />
            <field name="arch" type="xml">
                <xpath expr="//field[@name='active']" position="after">
                    <field name="purchase_year"/>
                    <field name="purchase_month"/>
                </xpath>
            </field>
        </record>
    </data>
</odoo>