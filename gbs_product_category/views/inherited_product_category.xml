<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data>
        <record id="inherit_consum_product_category_form" model="ir.ui.view">
            <field name="name">inherit.consum.product.category.form</field>
            <field name="model">product.category</field>
            <field name="inherit_id" ref="product.product_category_form_view"/>
            <field name="arch" type="xml">
                <xpath expr="//field[@name='type']" position="after">
                    <field name="is_backdateable" groups="stock.group_stock_manager"/>
                </xpath>
            </field>
        </record>
    </data>
</odoo>