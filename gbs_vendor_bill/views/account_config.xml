<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <record id="view_tds_vat_account_config" model="ir.ui.view">
        <field name="name">view.tds.vat.account.config</field>
        <field name="model">account.config.settings</field>
        <field name="inherit_id" ref="account.view_account_config_settings"/>
        <field name="arch" type="xml">
            <xpath expr="//group[@name='accounting']" position="after">
                <group string="Head Branch" name="head_branch">
                    <label for="id" string="Configuration" groups="account.group_account_user"/>
                    <div name="head_branch_id">
                        <div>
                            <label for="head_branch_id"/>
                            <field name="head_branch_id" options="{'no_create':True,'no_create_edit':True,'no_open':True}"
                                   class="oe_inline"/>
                        </div>
                    </div>
                </group>
            </xpath>
        </field>
    </record>
</odoo>