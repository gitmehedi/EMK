<?xml version="1.0" encoding="UTF-8"?>

<odoo noupdate="1">
    <record id="ir_cron_db_operation_manage" model="ir.cron">
        <field name="name">Database Sync Delete Scheduler</field>
        <field name="user_id" ref="base.user_root"/>
        <field name="active">True</field>
        <field name="interval_number">7</field>
        <field name="interval_type">days</field>
        <field name="numbercall">-1</field>
        <field name="priority">5</field>
        <field name="nextcall"
               eval="(datetime.now() + timedelta(days=1)).strftime('%Y-%m-%d 02:00:00')"/>
        <field name="model">db.operation.manage</field>
        <field name="function">_delete_db_result_line</field>
    </record>
</odoo>
