<?xml version="1.0" encoding="UTF-8"?>

<odoo noupdate="1">
    <record id="ir_cron_report_database_sync_scheduler" model="ir.cron">
        <field name="name">OGL-CBS Report Database Sync Scheduler</field>
        <field name="user_id" ref="base.user_root"/>
        <field name="interval_number">10</field>
        <field name="interval_type">minutes</field>
        <field name="numbercall">-1</field>
        <field name="priority">5</field>
        <field name="nextcall"
               eval="(datetime.now() + timedelta(days=1)).strftime('%Y-%m-%d 02:00:00')"/>
        <field name="model">db.operation.manage</field>
        <field name="function">act_report_sync</field>
    </record>
</odoo>
