<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data noupdate="0">
        <record id="module_category_tpm" model="ir.module.category">
            <field name="name">TPM Management</field>
            <field name="sequence">90</field>
        </record>
        <record id="grp_tpm_user" model="res.groups">
            <field name="name">Branch User</field>
            <field name="category_id" ref="module_category_tpm"/>
            <field name="implied_ids" eval="[(4, ref('base.group_user'))]"/>
            <field name="sequence">10</field>
        </record>
        <record id="grp_tpm_manager" model="res.groups">
            <field name="name">Group Finance</field>
            <field name="category_id" ref="module_category_tpm"/>
            <field name="implied_ids" eval="[(4, ref('grp_tpm_user'))]"/>
            <field name="sequence">20</field>
        </record>
        <record id="grp_tpm_system_settings" model="res.groups">
            <field name="name">System Settings</field>
            <field name="category_id" ref="module_category_tpm"/>
            <field name="implied_ids" eval="[(4, ref('grp_tpm_manager'))]"/>
            <field name="sequence">30</field>
        </record>
    </data>
</odoo>



