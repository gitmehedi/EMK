        <t t-foreach="docs" t-as="o">
            <t t-call="report.external_layout">
                <div class="page">
                    <h2>Attendance Summary</h2>
                    <p><span t-field="o.name"/></p>
                    <p><span t-field="o.period"/></p>
                    <p><span t-field="o.state"/></p>                    
                    <!--  <p><span t-field="o.line_ids"/></p> -->
                    
                </div>
            </t>
        </t>
    </t>
</template>
</odoo>
