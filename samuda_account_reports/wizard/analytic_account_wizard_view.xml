<?xml version="1.0" encoding="utf-8" ?>
<odoo>
	<data>
		<record id="view_analytic_account_wizard_form" model="ir.ui.view">
			<field name="name">analytic.account.wizard.form</field>
			<field name="model">analytic.account.wizard</field>
			<field name="arch" type="xml">
			    <form>
                    <group>
                        <label for="all_entries" string="Options"/>
                        <div name="options">
                            <div>
                                <field name="all_entries"/>
                                <label for="all_entries"/>
                            </div>
                        </div>
                    </group>
                    <group col="4">
                        <field name="cost_center_id" placeholder="All" options="{'no_create': True,'no_create_edit': True,'no_open': True}"/>
                        <field name="operating_unit_id" placeholder="All" options="{'no_create': True,'no_create_edit': True,'no_open': True}"/>
                        <field name="date_from"/>
                        <field name="date_to"/>
                    </group>
                    <footer>
                        <button name ="button_export_xlsx" string="Export" type="object" class="oe_highlight"/>
                        <button special="cancel" string="Cancel" />
                    </footer>
                </form>
			</field>
		</record>

		<act_window id="action_analytic_account_wizard"
                    key2="client_print_multi"
                    name="Analytic Report"
                    res_model="analytic.account.wizard"
                    src_model="account.analytic.account"
                    view_mode="form" target="new" view_type="form" />

    </data>
</odoo>