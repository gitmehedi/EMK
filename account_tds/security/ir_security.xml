<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data noupdate="0">

        <record id="module_category_tds" model="ir.module.category">
            <field name="name">VDS/TDS Deduction</field>
            <field name="sequence">80</field>
        </record>
        <record id="group_tds_user" model="res.groups">
            <field name="name">Accountant</field>
            <field name="category_id" ref="account_tds.module_category_tds"/>
            <field name="sequence">10</field>
        </record>
        <record id="group_tds_manager" model="res.groups">
            <field name="name">Adviser</field>
            <field name="category_id" ref="account_tds.module_category_tds"/>
            <field name="implied_ids" eval="[(4, ref('account_tds.group_tds_user'))]"/>
            <field name="sequence">20</field>
        </record>
        <record id="group_tds_system_settings" model="res.groups">
            <field name="name">System Settings</field>
            <field name="category_id" ref="account_tds.module_category_tds"/>
            <field name="implied_ids" eval="[(4, ref('account_tds.group_tds_manager'))]"/>
            <field name="sequence">30</field>
        </record>
    </data>
</odoo>