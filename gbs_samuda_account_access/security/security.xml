<?xml version="1.0" encoding="utf-8"?>
<odoo>

    <record id="group_account_reconciliation_button" model="res.groups">
        <field name="name">Account Reconciliation Button</field>
        <field name="category_id" ref="gbs_application_group.module_category_sale"/>
        <field name="sequence">30</field>
    </record>

    <record id="group_journal_item" model="res.groups">
        <field name="name">Journal Items</field>
        <field name="category_id" ref="gbs_application_group.module_category_sale"/>
        <field name="menu_access" eval="[(4, ref('account.menu_action_account_moves_all'))]"/>
        <field name="sequence">30</field>
    </record>

    <menuitem id="menu_account_unit_accounting"
                  name="Accounting" groups="account.group_account_user"
                  parent="gbs_samuda_account_access.menu_finance_unit"
                  sequence="2"/>
    <menuitem id="menu_account_unit_journal_items"
                  name="Journal Items" action="account.action_account_moves_all_a"
                  parent="gbs_samuda_account_access.menu_account_unit_accounting"
                  groups="account.group_account_user"
                  sequence="1"/>

    <menuitem id="menu_account_unit_journal_entries"
                  name="Journal Entries" action="account.action_move_journal_line"
                  parent="gbs_samuda_account_access.menu_account_unit_accounting"
                  groups="account.group_account_user"
                  sequence="2"/>




</odoo>

