<?xml version="1.0" encoding="utf-8"?>
<openerp>
	<data>
		<!-- Definition of the report (picking Move) -->
		<report id="report_stock_summary" 
		    	model="stock.move" 
		    	string="Stock Summary Report"
				name="stock_summary_report.report_stock_summary_qweb" 
				multi="True" 
				file="stock_summary_report.report_stock_summary_qweb"
				report_type="qweb-pdf" />
				
		<record id="paperformat_Landscape" model="report.paperformat">
		    <field name="name">Landscape Report</field>
		    <field name="orientation">Landscape</field>
		    <field name="format">A4</field>
		    <field name="header_line" eval="False"/>
		</record>
		
		<record id="report_stock_summary" model="ir.actions.report.xml">
            <field name="paperformat_id" ref="paperformat_Landscape"/>
        </record>
        		
	</data>
</openerp>