<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data noupdate="1">


        <record id="seq_member_paid" model="ir.sequence">
            <field name="name">Member Paid Sequence</field>
            <field name="code">res.partner.member</field>
            <field name="prefix">M</field>
            <field name="padding">7</field>
            <field name="company_id" eval="False"/>
        </record>

        <record id="sequence_payment_session" model="ir.sequence">
            <field name="name">Sequence Payment Session</field>
            <field name="code">payment.session.seq</field>
            <field name="prefix">PS/%(year)s/%(month)s/%(day)s/</field>
            <field eval="2" name="padding"/>
            <field eval="1" name="number_increment"/>
        </record>
    </data>
</odoo>