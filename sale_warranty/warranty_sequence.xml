        <!-- Sequences for sale.warranty.claim -->
        <record id="seq_type_warranty_claim" model="ir.sequence.type">
            <field name="name">Claim No</field>
            <field name="code">sale.warranty.claim</field>
        </record>

        <record id="seq_warr_claim" model="ir.sequence">
            <field name="name">Claim No</field>
            <field name="code">sale.warranty.claim</field>
            <field name="prefix">SWC</field>
            <field name="padding">3</field>
            <field name="company_id" eval="False"/>
        </record>

    </data>
</openerp>
