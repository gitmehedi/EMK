<?xml version="1.0" encoding="UTF-8"?>
<openerp>
    <data>
        <record id="view_wizard_all_branch_requisition_wizard" model="ir.ui.view">
            <field name="name">All Branch Requisition</field>
            <field name="model">all.branch.requisition.wizard</field>
            <field name="arch" type="xml">
                <form string="Branch Requistision">
                    <group>
                        <group>
                            <field name="period_id"
                                   options="{'no_quick_create': True, 'no_create_edit': True, 'no_open': True}"/>
                        </group>
                        <group></group>
                    </group>
                    <footer>
                        <button name="print_report" string="Print" type="object" class="oe_highlight"/>
                        or
                        <button string="Cancel" class="oe_link" special="cancel"/>
                    </footer>
                </form>
            </field>
        </record>


        <record id="action_wizard_all_branch_requisition" model="ir.actions.act_window">
            <field name="name">Product Requisition Summary</field>
            <field name="res_model">all.branch.requisition.wizard</field>
            <field name="view_type">form</field>
            <field name="view_mode">tree,form</field>
            <field name="view_id" ref="view_wizard_all_branch_requisition_wizard"/>
            <field name="target">new</field>
        </record>

        <menuitem id="menu_action_wizard_all_branch_requisition"
                  action="action_wizard_all_branch_requisition"
                  parent="stock.next_id_61"
                  sequence="15"/>

    </data>
</openerp>