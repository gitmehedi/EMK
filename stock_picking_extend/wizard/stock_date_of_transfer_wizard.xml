<?xml version="1.0" encoding="UTF-8"?>
<odoo>
    <record id="view_stock_date_of_transfer_form" model="ir.ui.view">
        <field name="name">stock.date.of.transfer.form</field>
        <field name="model">stock.date.transfer</field>
        <field name="arch" type="xml">
            <form string="Please Enter The Date Of Transfer">
                <group>
                    <group>
                        <field name="date_done" required="1"/>
                    </group>
                    <group></group>
                </group>
                <footer>
                    <button name="save" string="Save" type="object" class="btn-primary"/>
                    <button string="close" class="btn-default" special="cancel" />
                </footer>
            </form>
        </field>
    </record>
</odoo>