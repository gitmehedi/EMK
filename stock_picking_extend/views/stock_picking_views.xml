<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <record id="view_stock_picking_form1" model="ir.ui.view">
        <field name="name">stock.picking.form1</field>
        <field name="model">stock.picking</field>
        <field name="arch" type="xml">
            <form string="Receive Transfers">
                <header>
                    <button name="action_assign" states="confirmed,waiting" string="Reserve" type="object" class="oe_highlight" groups="stock.group_stock_user"/>
                    <button name="action_assign" states="partially_available" string="Recheck Availability" type="object" groups="stock.group_stock_user"/>
                    <button name="do_new_transfer" states="draft,partially_available,assigned" string="Receive" groups="stock.group_stock_user" type="object" class="oe_highlight"/>
                    <button name="%(stock.act_stock_return_picking)d" string="Return" states="done" type="action" groups="stock.group_stock_user"/>
                    <button name="action_cancel" states="assigned,confirmed,partially_available,draft,waiting" string="Cancel" groups="stock.group_stock_user" type="object"/>
                    <button name="do_prepare_partial" attrs="{'invisible': ['|', ('launch_pack_operations', '=', True), '|', ('state', 'not in', ('assigned', 'partially_available')), ('pack_operation_ids', '!=', [])]}"
                            string="Reset Operations" type="object"/>
                    <field name="launch_pack_operations" invisible="1"/>
                    <field name="state" widget="statusbar" statusbar_visible="draft,done" />
                </header>
                <sheet>
                    <div class="oe_button_box" name="button_box">
                        <button class="oe_stat_button" name="attachment_tree_view"  type="object" icon="fa-files-o">
                            <field string="Documents" name="doc_count" widget="statinfo"/>
                        </button>
                    </div>
                    <h1 class="hidden-xs">
                        <field name="name" attrs="{'invisible': [('name','=','/')]}" readonly="1"/>
                    </h1>
                    <group>
                        <group>
                            <field name="origin" placeholder="e.g. PO0032" class="oe_inline" attrs="{'readonly': [('picking_type_code', '=', 'internal')]}"/>
                            <field name="receive_type" placeholder="Please Select" attrs="{'invisible': [('picking_type_code', 'in', ['internal','outgoing'])]}"/>
                            <field name="transfer_type" invisible="1"/>
                            <field name="location_id" groups="stock.group_stock_multi_locations" attrs="{'invisible': [('picking_type_code', '=', 'outgoing')]}"/>
                            <field name="location_dest_id" groups="stock.group_stock_multi_locations" attrs="{'invisible': [('picking_type_code', '=', 'incoming')]}"/>
                            <field name="backorder_id" readonly="1" attrs="{'invisible': [('backorder_id','=',False)]}"/>
                        </group>
                        <group>
                            <field name="min_date"/>
                            <field name="partner_id" domain="[('supplier', '=', True),('parent_id', '=', False)]"
                                   attrs="{'invisible': [('picking_type_code', '=', 'internal')],'required':[('receive_type', 'in',['lc','other'])]}"/>
                            <field name="challan_bill_no" attrs="{'required':[('receive_type', 'in',['lc','other'])]}"/>
                            <field name="challan_date"/>
                            <field name="date_done"/>
                            <label for="owner_id" groups="stock.group_tracking_owner"/>
                            <div groups="stock.group_tracking_owner">
                                <field name="owner_id"/>
                                <button name="action_assign_owner" string="Assign Owner" type="object" attrs="{'invisible': ['|',('pack_operation_exist', '=', False),('state', 'not in', ('draft','assigned','confirmed'))]}"
                                        class="oe_link oe_edit_only"/>
                            </div>
                        </group>
                    </group>
                    <notebook>
                        <page string="Operations" attrs="{'invisible': [('state', 'in', ('draft', 'confirmed', 'waiting'))]}">
                            <button name="do_prepare_partial" type="object" string="Recompute" attrs="{'invisible': [('recompute_pack_op','=', False)]}"/>
                            <field name="recompute_pack_op" invisible="1"/>
                            <field name="pack_operation_ids" invisible="1"/>
                            <field name="pack_operation_product_ids" options="{'reload_on_button': True}" context="{'default_picking_id': id, 'default_location_id': location_id, 'default_location_dest_id': location_dest_id}" mode="tree,kanban">
                                <tree editable="bottom" decoration-muted="result_package_id" decoration-danger="qty_done&gt;product_qty" decoration-success="qty_done==product_qty and state!='done' and not result_package_id">
                                    <field name="package_id" groups="stock.group_tracking_lot" invisible="1"/>
                                    <field name="product_id" required="1" attrs="{'readonly': [('fresh_record', '=', False)]}"/>
                                    <field name="fresh_record" invisible="1"/>
                                    <field name="product_uom_id" attrs="{'readonly': [('fresh_record', '=', False)]}" groups="base.group_user"/>
                                    <field name="lots_visible" invisible="1"/>
                                    <field name="owner_id" groups="stock.group_tracking_owner"/>
                                    <field name="location_id" domain="[('id', 'child_of', parent.location_id)]" invisible="1"/>
                                    <field name="location_dest_id" domain="[('id', 'child_of', parent.location_dest_id)]" invisible="1"/>
                                    <field name="from_loc" groups="stock.group_stock_multi_locations,stock.group_tracking_lot"/>
                                    <field name="to_loc" groups="stock.group_stock_multi_locations,stock.group_tracking_lot"/>
                                    <field name="result_package_id" groups="stock.group_tracking_lot" context="{'default_location_id': location_dest_id}" invisible="1"/>
                                    <field name="state" invisible="1"/>
                                    <field name="product_qty" readonly="1" attrs="{'required': [('product_id', '!=', False)]}"/>
                                    <field name="qty_done" attrs="{'readonly': [('lots_visible', '=', True)]}"/>
                                    <button name="split_lot" string="Lot Split" type="object" icon="fa-list" groups="stock.group_production_lot"
                                            attrs="{'invisible': ['|', ('lots_visible', '=', False), ('state', 'not in', ['confirmed', 'assigned', 'waiting', 'partially_available','done'])]}"/>
                                    <button name="show_details" string="Modify" type="object" icon="fa-pencil" groups="stock.group_stock_multi_locations"
                                            states="confirmed,assigned,waiting,partially_available"/>
                                </tree>
                                <kanban class="o_kanban_mobile">
                                    <field name="package_id" groups="stock.group_tracking_lot" invisible="1"/>
                                    <field name="product_id" required="1" attrs="{'readonly': [('fresh_record', '=', False)]}"/>
                                    <field name="fresh_record" invisible="1"/>
                                    <field name="product_uom_id" attrs="{'readonly': [('fresh_record', '=', False)]}" groups="product.group_uom"/>
                                    <field name="lots_visible" invisible="1"/>
                                    <field name="owner_id" groups="stock.group_tracking_owner"/>
                                    <field name="location_id" domain="[('id', 'child_of', parent.location_id)]" invisible="1"/>
                                    <field name="location_dest_id" domain="[('id', 'child_of', parent.location_dest_id)]" invisible="1"/>
                                    <field name="from_loc" groups="stock.group_stock_multi_locations,stock.group_tracking_lot"/>
                                    <field name="to_loc" groups="stock.group_stock_multi_locations,stock.group_tracking_lot"/>
                                    <field name="result_package_id" groups="stock.group_tracking_lot" context="{'default_location_id': location_dest_id}" invisible="1"/>
                                    <field name="state" invisible="1"/>
                                    <field name="product_qty" readonly="1" attrs="{'required': [('product_id', '!=', False)]}"/>
                                    <field name="qty_done" attrs="{'readonly': [('lots_visible', '=', True)]}"/>
                                    <templates>
                                        <t t-name="kanban-box">
                                            <div t-attf-class="oe_kanban_card oe_kanban_global_click
                                                               #{record.qty_done.raw_value&gt;record.product_qty.raw_value ? 'oe_kanban_color_2' : ''}
                                                               #{record.qty_done.raw_value==record.product_qty.raw_value &amp;&amp; record.state.raw_value!='done' &amp;&amp; ! record.result_package_id.raw_value ? 'oe_kanban_color_3' : ''}">
                                                <div class="row">
                                                    <div class="col-xs-12">
                                                        <strong><span><t t-esc="record.product_id.value"/></span></strong>
                                                    </div>
                                                </div>
                                                <div class="row">
                                                    <div class="col-xs-12 text-muted">
                                                        <span><t t-esc="record.from_loc.value"/> to <t t-esc="record.to_loc.value"/></span>
                                                    </div>
                                                </div>
                                                <div class="row">
                                                    <div class="col-xs-12 text-muted">
                                                        <span><t t-esc="record.qty_done.value"/> of <t t-esc="record.product_qty.value"/> <t t-esc="record.product_uom_id.value"/></span>
                                                    </div>
                                                </div>
                                            </div>
                                        </t>
                                    </templates>
                                </kanban>
                            </field>
                            <field name="picking_type_entire_packs" invisible="1"/>
                            <field name="pack_operation_pack_ids" options="{'reload_on_button': True}" attrs="{'invisible': [('pack_operation_pack_ids', '=', []), ('picking_type_entire_packs', '=', False)]}" context="{'default_picking_id': id, 'default_location_id': location_id, 'default_location_dest_id': location_dest_id, 'default_picking_id': id}" groups="stock.group_tracking_lot">
                                <tree editable="bottom" decoration-muted="qty_done&gt;0">
                                    <field name="package_id" required="1" string="Package To Move" domain="[('location_id', '=', location_id)]"/>
                                    <field name="location_id" domain="[('id', 'child_of', parent.location_id)]" invisible="1"/>
                                    <field name="from_loc" groups="stock.group_stock_multi_locations,stock.group_tracking_lot"/>
                                    <field name="to_loc" groups="stock.group_stock_multi_locations,stock.group_tracking_lot"/>
                                    <field name="location_dest_id" domain="[('id', 'child_of', parent.location_dest_id)]" invisible="1"/>
                                    <field name="result_package_id" groups="stock.group_tracking_lot" context="{'default_location_id': location_dest_id}" invisible="1"/>
                                    <field name="state" invisible="1"/>
                                    <field name="qty_done" invisible="1"/>
                                    <field name="lots_visible" invisible="1"/>
                                    <field name="is_done"/>
                                    <button name="show_details" string="Modify" type="object" icon="fa-pencil"
                                            states="confirmed,assigned,waiting,partially_available" groups="base.group_no_one"/>
                                </tree>
                            </field>
                            <button class="oe_link oe_right" name="put_in_pack" type="object" string="&#8627;Put in Pack" attrs="{'invisible': [('state', 'in', ('done', 'cancel'))]}" groups="stock.group_tracking_lot"/>
                        </page>
                        <page string="Initial Demand">
                            <field name="move_lines" attrs="{'readonly': [('state', 'in', ('cancel', 'partially_available', 'assigned', 'done'))]}" options="{'reload_on_button': True}" mode="tree,kanban" context="{'address_in_id': partner_id, 'form_view_ref':'stock.view_move_picking_form', 'tree_view_ref':'stock.view_move_picking_tree', 'default_picking_type_id': picking_type_id, 'default_location_id': location_id, 'default_location_dest_id': location_dest_id}">
                                <kanban class="o_kanban_mobile">
                                    <field name="product_id"/>
                                    <field name="availability"/>
                                    <field name="product_uom_qty" />
                                    <field name="product_uom" options="{'no_open': True, 'no_create': True}"/>
                                    <field name="scrapped"/>
                                    <field name="state"/>
                                    <templates>
                                        <t t-name="kanban-box">
                                            <div t-attf-class="oe_kanban_card oe_kanban_global_click">
                                                <div class="row">
                                                    <div class="col-xs-12">
                                                        <strong><span><t t-esc="record.product_id.value"/></span></strong>
                                                    </div>
                                                </div>
                                                <div class="row">
                                                    <div class="col-xs-8 text-muted">
                                                        <span><t t-esc="record.availability.value"/> on <t t-esc="record.product_uom_qty.value"/> <t t-esc="record.product_uom.value"/></span>
                                                    </div>
                                                    <div class="col-xs-4 text-muted">
                                                        <span class="pull-right text-right"><t t-esc="record.location_dest_id.value"/></span>
                                                    </div>
                                                </div>
                                            </div>
                                        </t>
                                    </templates>
                                </kanban>
                            </field>
                            <field name="pack_operation_exist" invisible="1"/>
                            <field name="note" placeholder="Add an internal note that will be printed on the Picking Operations sheet" class="oe_inline"/>
                        </page>
                        <page string="Additional Info" name="extra">
                            <group>
                                <group>
                                    <field name="move_type" readonly="1"/>
                                    <field name="picking_type_id" readonly="1"/>
                                    <field name="picking_type_code" invisible="1"/>
                                    <field name="quant_reserved_exist" invisible="1"/>
                                </group>
                                <group>
                                    <field name="company_id" groups="base.group_multi_company" options="{'no_create': True}"/>
                                    <field name="operating_unit_id" groups="operating_unit.group_multi_operating_unit" options="{'no_create': True}"/>
                                    <field name="group_id" readonly="1"/>
                                    <field name="priority" readonly="1"/>
                                </group>
                            </group>
                        </page>
                    </notebook>
                </sheet>
                <div class="oe_chatter">
                    <field name="message_follower_ids" widget="mail_followers"/>
                    <field name="message_ids" widget="mail_thread"/>
                </div>
            </form>
        </field>
    </record>

    <record id="view_stock_picking_tree1" model="ir.ui.view">
        <field name="name">view.picking.tree1</field>
        <field name="model">stock.picking</field>
        <field name="arch" type="xml">
            <tree string="Receive Transfers">
                <field name="name"/>
                <field name="location_dest_id"/>
                <field name="partner_id"/>
                <field name="date" invisible="1"/>
                <field name="min_date"/>
                <field name="origin"/>
                <field name="group_id" invisible="1"/>
                <field name="backorder_id"/>
                <field name="state"/>
                <field name="priority" invisible="1"/>
                <field name="picking_type_id" invisible="1"/>
            </tree>
        </field>
    </record>


    <record id="view_stock_picking_tree2" model="ir.ui.view">
        <field name="name">view.picking.tree2</field>
        <field name="model">stock.picking</field>
        <field name="arch" type="xml">
            <tree string="Receive Transfers" create="false" edit="false">
                <field name="name"/>
                <field name="location_dest_id"/>
                <field name="date" invisible="1"/>
                <field name="min_date"/>
                <field name="origin"/>
                <field name="group_id" invisible="1"/>
                <field name="backorder_id"/>
                <field name="state"/>
                <field name="priority" invisible="1"/>
                <field name="picking_type_id" invisible="1"/>
            </tree>
        </field>
    </record>

    <!--Search View-->
    <record id="search_stock_picking1" model="ir.ui.view">
        <field name="name">stock.picking.search1</field>
        <field name="model">stock.picking</field>
        <field name="arch" type="xml">
            <search string="Receipts Picking">
                <field name="name"/>
                <field name="partner_id"/>
                <field name="origin"/>
                <filter name="draft" string="Draft" domain="[('state','=','draft')]"/>
                <separator/>
                <filter name="ready" string="Ready" domain="[('state','in',('assigned', 'partially_available'))]"/>
            </search>
        </field>
    </record>
    <!--Search View-->
    <record id="search_stock_picking2" model="ir.ui.view">
        <field name="name">stock.picking.search2</field>
        <field name="model">stock.picking</field>
        <field name="arch" type="xml">
            <search string="Internal Picking">
                <field name="name"/>
                <field name="origin"/>
                <filter name="ready" string="Ready" domain="[('state','in',('assigned', 'partially_available'))]"/>
            </search>
        </field>
    </record>

    <!--Receive Goods Action -->
    <record model="ir.actions.act_window" id="action_stock_picking_receive1">
        <field name="name">Receive Goods</field>
        <field name="type">ir.actions.act_window</field>
        <field name="res_model">stock.picking</field>
        <field name="view_type">form</field>
        <field name="view_id" ref="view_stock_picking_tree1"/>
        <field name="search_view_id" ref="search_stock_picking1"/>
        <field name="context">{
            'search_default_ready':1,
            'default_transfer_type': 'receive',
            }
        </field>
        <field name="domain">[('location_dest_id.name','=','Input')]</field>
        <field name="help" type="html">
            <p>
                Here you can see Received Goods list.
            </p>
        </field>
        <!--<field name="context">{-->
        <!--'search_default_receive_filter': 1,}</field>-->
        <!--<field name="domain">[('transfer_type', '=', 'receive')]</field>-->
        <!--<field name="context">{'default_transfer_type': 'receive'}</field>-->
    </record>

    <!--Accounts Approver Action -->
    <record model="ir.actions.act_window" id="action_stock_picking_receive_acc_approver">
        <field name="name">Accounts Approver</field>
        <field name="type">ir.actions.act_window</field>
        <field name="res_model">stock.picking</field>
        <field name="view_type">form</field>
        <field name="view_id" ref="view_stock_picking_tree2"/>
        <field name="domain">[('check_ac_approve_button','=',True)]</field>
    </record>

    <!--Quality Control Action -->
    <record model="ir.actions.act_window" id="action_stock_picking_receive_qc">
        <field name="name">Quality Control</field>
        <field name="type">ir.actions.act_window</field>
        <field name="res_model">stock.picking</field>
        <field name="view_type">form</field>
        <field name="view_id" ref="view_stock_picking_tree2"/>
        <field name="domain">[('location_dest_id.name','=','Quality Control')]</field>
        <field name="search_view_id" ref="search_stock_picking2"/>
        <field name="context">{
            'search_default_ready':1,
            }
        </field>
        <!--<field name="context">{'default_transfer_type': 'receive'}</field>-->
    </record>

    <!--Sound Stock Action -->
    <record model="ir.actions.act_window" id="action_stock_picking_receive_sound_stock">
        <field name="name">Sound Stock</field>
        <field name="type">ir.actions.act_window</field>
        <field name="res_model">stock.picking</field>
        <field name="view_type">form</field>
        <field name="view_id" ref="view_stock_picking_tree2"/>
        <field name="domain">[('location_dest_id.name','=','Stock')]</field>
        <field name="search_view_id" ref="search_stock_picking2"/>
        <field name="context">{
            'search_default_ready':1,
            }
        </field>
        <!--<field name="context">{'default_transfer_type': 'receive'}</field>-->
    </record>

    <record id="action_stock_picking_view1" model="ir.actions.act_window.view">
        <field eval="201" name="sequence"/>
        <field name="view_mode">tree</field>
        <field name="view_id" ref="view_stock_picking_tree1"/>
        <field name="act_window_id" ref="action_stock_picking_receive1"/>
    </record>

    <record id="action_stock_picking_view2" model="ir.actions.act_window.view">
        <field eval="202" name="sequence"/>
        <field name="view_mode">form</field>
        <field name="view_id" ref="view_stock_picking_form1"/>
        <field name="act_window_id" ref="action_stock_picking_receive1"/>
    </record>

    <!--Receive Goods menu -->
    <menuitem id="menu_receive_picking"
              name="Receive Goods"
              parent="stock.menu_stock_warehouse_mgmt"
              sequence="2"
              action="action_stock_picking_receive1" groups="stock.group_stock_user"/>

    <!--Quality Control Menu -->
    <menuitem id="menu_receive_picking_qc"
              name="Quality Control"
              parent="stock.menu_stock_warehouse_mgmt"
              sequence="3"
              action="action_stock_picking_receive_qc" groups="stock.group_stock_user"/>

    <!--Sound Stock Menu -->
    <menuitem id="menu_receive_picking_sound_stock"
              name="Sound Stock"
              parent="stock.menu_stock_warehouse_mgmt"
              sequence="4"
              action="action_stock_picking_receive_sound_stock" groups="stock.group_stock_user"/>

    <!--Accounts Approver menu -->
    <menuitem id="menu_receive_picking_acc_approver"
              name="Inventory"
              parent="account.menu_finance"
              sequence="3"/>

    <menuitem id="menu_acc_approver"
              name="Accounts Approver" sequence="1"
              parent="menu_receive_picking_acc_approver"
              action="action_stock_picking_receive_acc_approver" groups="account.group_account_user"/>

    <record model="ir.ui.view" id="view_picking_form_inherit">
        <field name="name">stock.picking.inherit.form</field>
        <field name="model">stock.picking</field>
        <field name="inherit_id" ref="stock.view_picking_form"/>
        <field name="arch" type="xml">
            <xpath expr="//button[@name='action_see_move_scrap']" position="before">
                <button class="oe_stat_button" name="attachment_tree_view"  type="object" icon="fa-files-o">
                    <field string="Documents" name="doc_count" widget="statinfo"/>
                </button>
            </xpath>
            <!--<xpath expr="//header/button[@name='do_new_transfer']" position="replace">-->
            <!--<button name="do_new_transfer" string="Receive" groups="stock.group_stock_user"-->
            <!--states="draft,partially_available,assigned"-->
            <!--type="object" class="oe_highlight"/>-->
            <!--</xpath>-->
            <!--<xpath expr="//header/button[@name='force_assign']" position="attributes">-->
                <!--<attribute name="groups">base_technical_features.group_technical_features</attribute>-->
            <!--</xpath>-->
            <xpath expr="//header/button[@name='action_confirm']" position="attributes">
                <attribute name="groups">base_technical_features.group_technical_features</attribute>
            </xpath>

            <xpath expr="//header/button[@name='%(stock.act_stock_return_picking)d']" position="replace">
                <button name="%(stock.act_stock_return_picking)d" string="Return" states="done"
                        type="action" groups="stock.group_stock_user"/>
            </xpath>

            <!--<xpath expr="//header/button[@name='do_print_picking']" position="attributes">-->
            <!--<attribute name="invisible">1</attribute>-->
            <!--</xpath>-->

            <xpath expr="//header/button[@name='%(stock.action_report_delivery)d']" position="attributes">
                <attribute name="invisible">1</attribute>
            </xpath>

            <xpath expr="//header/button[@name='button_scrap']" position="attributes">
                <attribute name="invisible">1</attribute>
            </xpath>

            <xpath expr="//header/field[@name='state']" position="replace">
                <field name="state" widget="statusbar" statusbar_visible="draft,assigned,done"/>
            </xpath>

            <xpath expr="//sheet/notebook/page/field/tree/field[@name='product_uom_id']" position="replace">
                <field name="product_uom_id" attrs="{'readonly': [('fresh_record', '=', False)]}"
                       groups="base.group_user"/>
            </xpath>

            <xpath expr="//field[@name='move_type']" position="attributes">
                <attribute name="readonly">1</attribute>
            </xpath>

            <xpath expr="//field[@name='picking_type_id']" position="attributes">
                <attribute name="readonly">1</attribute>
            </xpath>

            <xpath expr="//field[@name='group_id']" position="attributes">
                <attribute name="readonly">1</attribute>
            </xpath>

            <xpath expr="//field[@name='priority']" position="attributes">
                <attribute name="readonly">1</attribute>
            </xpath>

            <xpath expr="//field[@name='partner_id']" position="replace">
                <field name="partner_id" options="{'no_open': True, 'no_create': True}"
                       attrs="{'invisible': [('picking_type_code', '=', 'internal')]}"/>
            </xpath>

            <xpath expr="//field[@name='partner_id']" position="after">
                <field name="challan_bill_no" attrs="{'invisible': [('picking_type_code', '!=', 'incoming')]}"/>
                <field name="challan_date" attrs="{'invisible': [('picking_type_code', '!=', 'incoming')]}"/>
            </xpath>

        </field>
    </record>

    <record id="view_move_picking_tree1" model="ir.ui.view">
        <field name="name">stock.move.tree1</field>
        <field name="model">stock.move</field>
        <field name="inherit_id" ref="stock_operating_unit.view_move_picking_tree"/>
        <field name="arch" type="xml">
            <xpath expr="//field[@name='operating_unit_id']" position="attributes">
                <attribute name="groups">base_technical_features.group_technical_features</attribute>
            </xpath>
            <xpath expr="//field[@name='operating_unit_dest_id']" position="attributes">
                <attribute name="groups">base_technical_features.group_technical_features</attribute>
            </xpath>
        </field>
    </record>

    <record id="view_stock_picking_type_search" model="ir.ui.view">
        <field name="name">stock.picking.type.search</field>
        <field name="model">stock.picking.type</field>
        <field name="arch" type="xml">
            <search string="Stock Picking Type">
                <field name="show_on_dashboard"/>
                <filter string="Favorites" name="dashboard" domain="[('show_on_dashboard', '=', True)]"/>
            </search>
        </field>
    </record>

    <record id="stock.stock_picking_type_action" model="ir.actions.act_window">
        <field name="name">Inventory</field>
        <field name="res_model">stock.picking.type</field>
        <field name="type">ir.actions.act_window</field>
        <field name="view_type">form</field>
        <field name="view_mode">kanban,form</field>
        <field name="search_view_id" ref="view_stock_picking_type_search"/>
        <field name="context">{'search_default_dashboard':1}</field>
        <field name="help" type="html">
            <p class="oe_view_nocontent_create">
                Click to create a new picking type.
            </p><p>
            The picking type system allows you to assign each stock
            operation a specific type which will alter its views accordingly.
            On the picking type you could e.g. specify if packing is needed by default,
            if it should show the customer.
        </p>
        </field>
    </record>

    <record id="view_picking_type_form1" model="ir.ui.view">
        <field name="name">picking.type.form1</field>
        <field name="model">stock.picking.type</field>
        <field name="inherit_id" ref="stock.view_picking_type_form"/>
        <field name="arch" type="xml">
            <xpath expr="//form/sheet/group[2]" position="after">
                <group>
                    <field name="show_on_dashboard" groups="base.group_system"/>
                </group>
            </xpath>
        </field>
    </record>

    <record id="view_move_picking_tree1" model="ir.ui.view">
        <field name="name">stock.move.product.price.tree</field>
        <field name="model">stock.move</field>
        <field name="inherit_id" ref="stock.view_move_picking_tree"/>
        <field eval="16" name="priority"/>
        <field name="arch" type="xml">
            <field name="product_id" position="after">
                <field name="price_unit" invisible="1"/>
            </field>
        </field>
    </record>

</odoo>