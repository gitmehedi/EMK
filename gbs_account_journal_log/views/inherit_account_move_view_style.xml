<?xml version="1.0" encoding="UTF-8"?>
<odoo>
    <data>
        <template id="assets_backend" name="gbs_account_journal_log assets" inherit_id="web.assets_backend">
            <xpath expr="." position="inside">
                <link href="/gbs_account_journal_log/static/src/css/account_move.css" rel="stylesheet"/>
            </xpath>
        </template>

        <record id="view_inherit_account_move_form_1" model="ir.ui.view">
            <field name="name">inherit.account.move.form</field>
            <field name="model">account.move</field>
            <field name="inherit_id" ref="account.view_move_form"/>
            <field name="arch" type="xml">
                <xpath expr="//form" position="attributes">
                    <attribute name="class">gbs_journal</attribute>
                </xpath>
                <xpath expr="//tree" position="attributes">
                    <attribute name="class">gbs_journal_tree</attribute>
                </xpath>
            </field>
        </record>


    </data>
</odoo>