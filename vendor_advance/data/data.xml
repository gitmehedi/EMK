<?xml version='1.0' encoding='UTF-8'?>
<odoo>
    <data noupdate="1">

        <record id="vendor_advance_journal" model="account.journal">
            <field name="name">Vendor Advance</field>
            <field name="type">purchase</field>
            <field name="code">VAJ</field>
        </record>

        <!-- Sequence Records -->
        <record id="seq_vendor_advance" model="ir.sequence">
            <field name="name">Vendor Advances</field>
            <field name="code">vendor.advance</field>
            <field name="prefix">VA/%(range_year)s/</field>
            <field eval="1" name="number_next"/>
            <field eval="1" name="number_increment"/>
            <field name="padding">5</field>
            <field name="implementation">no_gap</field>
        </record>
        <record id="seq_vendor_security_return" model="ir.sequence">
            <field name="name">Vendor Security Return</field>
            <field name="code">vendor.security.return</field>
			<field name="active">TRUE</field>
            <field name="prefix">VSR/%(range_year)s/</field>
            <field name="padding">5</field>
			<field name="number_increment">1</field>
            <field name="implementation">no_gap</field>
        </record>

    </data>
</odoo>