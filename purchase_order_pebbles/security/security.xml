<?xml version="1.0" encoding="utf-8"?>
<openerp>
    <data noupdate="0">
        <record id="warehouse_group_user_admin" model="res.groups">
            <field name="name">Warehouse Admin</field>
            <field name="category_id" ref="base.module_category_warehouse_management"/>
        </record>

        <record model="ir.rule" id="goods_receive_matrix_request_comp_rule">
            <field name="name">Goods Receive Matrix multi-company</field>
            <field name="model_id" ref="model_goods_receive_matrix"/>
            <field name="global" eval="True"/>
            <field name="domain_force">['|',('company_id','=',False),('company_id','child_of',[user.company_id.id])]
            </field>
        </record>
        <record model="ir.rule" id="report_pos_order_analysis">
            <field name="name">Report PoS Order Analysis</field>
            <field name="model_id" ref="point_of_sale.model_report_pos_order"/>
            <field name="global" eval="True"/>
            <field name="domain_force">[('location_id.operating_unit_id','=',[rec.id for rec in user.operating_unit_ids])]
            </field>
        </record>

    </data>
</openerp>
