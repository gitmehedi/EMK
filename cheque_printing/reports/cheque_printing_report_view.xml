<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data>
        <report
                id="cheque_print_report"
                model="cheque.info.entry"
                string="Cheque Print"
                report_type="qweb-pdf"
                name="cheque_printing.report_cheque_prints_gbs"
                file="cheque_printing.report_cheque_prints_gbs"
        />

        <template id="report_cheque_prints_gbs">
            <t t-call="report.html_container">
                <t t-foreach="docs" t-as="o">
                    <div class="page">
                        <div class="row">
                            <div class="col-xs-8">
                                <span t-field="o.partner_id"/>
                                <span t-field="o.cheque_number"/>
                                <span t-field="o.cheque_date"/>
                                <span t-field="o.amount"/>
                            </div>
                        </div>
                    </div>
                </t>
            </t>
        </template>
    </data>
</odoo>