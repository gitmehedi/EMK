<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data>
        <record id="analytic_account_wizard_form" model="ir.ui.view">
            <field name="name">analytic,account.wizard.form</field>
            <field name="model">analytic.account.selection.wizard</field>
            <field name="arch" type="xml">
                <form>
                        <group>
                            <field name="analytic_account"/>
                        </group>
                        <group col="4">
                            <field name="expense_type"/>
                            <field name="ref"/>
                        </group>



                    <footer>
                        <button name="action_import" string="Import" type="object" default_focus="1"
                                class="oe_highlight"/>
                        <button name="action_cancel" string="Cancel" type="object" default_focus="1"
                                class="btn-danger"/>
                    </footer>
                </form>
            </field>
        </record>


    </data>
</odoo>