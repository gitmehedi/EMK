<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data>
        <record id="landed_cost_report_wizard_form" model="ir.ui.view">
            <field name="name">landed.cost.report.wizard.form</field>
            <field name="model">landed.cost.report.wizard</field>
            <field name="arch" type="xml">
                <form>
                    <group col="4">
                        <field name="product_id" string="Product"/>
                        <field name="landed_cost_id" invisible="1"/>
                    </group>
                    <footer>
                        <button name="print_landed_cost_xls" string="Export" type="object" default_focus="1"
                                class="oe_highlight"/>

                        <button string="Cancel" class="oe_link" special="cancel"/>
                    </footer>
                </form>
            </field>
        </record>


    </data>
</odoo>