<?xml version="1.0" encoding="UTF-8"?>
<odoo>
    <data>
        <record id="module_category_cheque_received" model="ir.module.category">
            <field name="name">Cheque Receive Access</field>
            <field name="sequence">202</field>
        </record>

        <record id="group_cheque_receive_unit" model="res.groups">
            <field name="name">Cheque Receive(Unit)</field>
            <field name="category_id" ref="module_category_cheque_received"/>
            <field name="sequence">101</field>
        </record>

        <record id="group_cheque_receive_ho" model="res.groups">
            <field name="name">Cheque Receive(Head Office)</field>
            <field name="category_id" ref="module_category_cheque_received"/>
            <field name="sequence">102</field>
             <field name="implied_ids" eval="[(4, ref('group_cheque_receive_unit'))]"/>
        </record>
        <record id="group_cheque_receive_adviser" model="res.groups">
            <field name="name">Cheque Receive(Adviser)</field>
            <field name="category_id" ref="module_category_cheque_received"/>
            <field name="sequence">103</field>
             <field name="implied_ids" eval="[(4, ref('group_cheque_receive_ho'))]"/>
        </record>
    </data>
</odoo>