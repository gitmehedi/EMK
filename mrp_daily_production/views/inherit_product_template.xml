<?xml version="1.0" encoding="UTF-8"?>

<odoo>
    <data>
        <record id="product_template_inherit_view" model="ir.ui.view">
            <field name="name">product.template.inherit.view</field>
            <field name="model">product.template</field>
            <field name="inherit_id" ref="product.product_template_form_view"/>
            <field name="arch" type="xml">
                <xpath expr="//field[@name='sale_ok']" position="replace">
                    <div>
                        <field name="finish_good"/>
                        <label for="finish_good"/>
                    </div>
                    <div>
                        <field name="consumable"/>
                        <label for="consumable"/>
                    </div>
                </xpath>
            </field>
        </record>
    </data>
</odoo>