<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data noupdate="1">
        <!-- Sequences for service.order -->
        <record id="commission_claim_sequence_SCCL_HO" model="ir.sequence">
            <field name="name">COMM SCCL HO</field>
            <field name="code">commission.claim</field>
            <field name="prefix">COMM-OU-%(year)s-</field>
            <field eval="6" name="padding"/>
            <field eval="1" name="number_increment"/>
            <field eval="False" name="company_id"/>
        </record>

        <record id="commission_claim_sequence_SCCL_DF" model="ir.sequence">
