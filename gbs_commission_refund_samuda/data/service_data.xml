            <field name="prefix">COMM-OU-%(year)s-</field>
            <field eval="6" name="padding"/>
            <field eval="1" name="number_increment"/>
            <field eval="False" name="company_id"/>
        </record>

        <!--sequence for refund claim-->
        <record id="refund_claim_sequence_SCCL_HO" model="ir.sequence">
            <field name="name">REF SCCL HO</field>
            <field name="code">refund.claim</field>
            <field name="prefix">REF-OU-%(year)s-</field>
