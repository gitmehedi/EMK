<openerp>
    <data>
        <record id="inherited_pos_config_forms" model="ir.ui.view">
			<field name="name">inherited.pos.config.form</field>
			<field name="model">pos.config</field>
			<field name="priority" eval="20" />
			<field name="inherit_id" ref="point_of_sale.view_pos_config_form" />
			<field name="arch" type="xml">
				<data>
					<xpath expr="//field[@name='journal_id']" position="after">
						<field name="active_shop" />
					</xpath>
                </data>
			</field>
		</record>
        <record id="inherited_pos_config_tree" model="ir.ui.view">
			<field name="name">inherited.pos.config.tree</field>
			<field name="model">pos.config</field>
			<field name="priority" eval="20" />
			<field name="inherit_id" ref="point_of_sale.view_pos_config_tree" />
			<field name="arch" type="xml">
				<data>
					<xpath expr="//field[@name='state']" position="before">
						<field name="active_shop" />
					</xpath>
                </data>
			</field>
		</record>
    </data>
</openerp>