<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data>
        <record id="sequence_item_borrowing" model="ir.sequence">
            <field name="name">Item Borrowing</field>
            <field name="code">item.borrowing</field>
            <field name="prefix">ITEM BORROWING-%(year)s-</field>
            <field eval="6" name="padding"/>
            <field eval="1" name="number_increment"/>
            <field eval="False" name="company_id"/>
        </record>

        <record id="sequence_item_loan_lending" model="ir.sequence">
            <field name="name">Item Loan Lending</field>
            <field name="code">item.loan.lending</field>
            <field name="prefix">LOAN LENDING-%(year)s-</field>
            <field eval="6" name="padding"/>
            <field eval="1" name="number_increment"/>
            <field eval="False" name="company_id"/>
        </record>
    </data>
</odoo>