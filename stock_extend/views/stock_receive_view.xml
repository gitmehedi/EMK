<?xml version="1.0" encoding="UTF-8"?>
<openerp>
	<data>
		
		<record id='action_stock_receive' model='ir.actions.act_window'>
			<field name="name">Good Receive</field>
			<field name="res_model">stock.picking</field>
			<field name="view_type">form</field>
			<field name="view_mode">tree,form</field>
			<field eval="False" name="view_id" />
			<field name="domain">[('good_receive_flag','=',True),('qc_receive_flag','=',False),('state','=','assigned')]</field>
		</record>
		
		<menuitem action="action_stock_receive" name="Good Receive"
			parent="sub_group_menu_indent_stock_transfer" id="menu_stock_receive"
			sequence="4" groups="stock.group_stock_user" />
		
		<record id='action_qc_receive' model='ir.actions.act_window'>
			<field name="name">QC Receive</field>
			<field name="res_model">stock.picking</field>
			<field name="view_type">form</field>
			<field name="view_mode">tree,form</field>
			<field eval="False" name="view_id" />
			<field name="domain">[('good_receive_flag','=',False),('qc_receive_flag','=',True),('state','in',('assigned','partially_available'))]</field>
		</record>
		
		<menuitem action="action_qc_receive" name="QC Receive"
			parent="sub_group_menu_indent_stock_transfer" id="menu_qc_receive"
			sequence="5" groups="stock.group_stock_user" />
		
		<record id='action_qc_pass' model='ir.actions.act_window'>
			<field name="name">QC Pass</field>
			<field name="res_model">stock.picking</field>
			<field name="view_type">form</field>
			<field name="view_mode">tree,form</field>
			<field eval="False" name="view_id" />
			<field name="domain">[('good_receive_flag','=',False),('qc_receive_flag','=',False),('qc_pass_flag','=',True),('state','in',('assigned','partially_available'))]</field>
		</record>
		
		<menuitem action="action_qc_pass" name="QC Pass"
			parent="sub_group_menu_indent_stock_transfer" id="menu_qc_pass"
			sequence="6" groups="stock.group_stock_user" />	
		
	</data>
</openerp>