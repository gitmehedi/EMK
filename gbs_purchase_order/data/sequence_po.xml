<odoo>
    <data>
        <!-- Sequences for purchase.quotation -->
        <record id="sequence_purchase_quotation" model="ir.sequence">
            <field name="name">Purchase Order</field>
            <field name="code">purchase.quotation</field>
            <field name="prefix">PQ-OU-%(year)s-</field>
            <field name="padding">6</field>
            <field name="company_id" eval="False"/>
        </record>
        <!-- Sequences for purchase.order -->
        <record id="sequence_purchase_order" model="ir.sequence">
            <field name="name">Purchase Quotation</field>
            <field name="code">purchase.order</field>
            <field name="prefix">PO-OU-%(year)s-</field>
            <field name="padding">6</field>
            <field name="company_id" eval="False"/>
        </record>

    </data>
</odoo>