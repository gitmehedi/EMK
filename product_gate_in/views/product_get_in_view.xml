<odoo>
    <data>

        <!-- Tree views on models -->
        <record model="ir.ui.view" id="product_gate_in_list_view">
            <field name="name">product.gate.in.tree.view</field>
            <field name="model">product.gate.in</field>
            <field name="type">tree</field>
            <field name="arch" type="xml">
                <tree string="Product Get In Form" decoration-info="state=='draft'" edit="false">
                    <field name="name"/>
                    <field name="ship_id"/>
                    <field name="challan_bill_no"/>
                    <field name="received"/>
                    <field name="operating_unit_id"/>
                    <field name="date"/>
                    <field name="state"/>
                </tree>
            </field>
        </record>

        <!-- Form views on models -->
        <record model="ir.ui.view" id="product_gate_in_form_view">
            <field name="name">product.gate.in.form.view</field>
            <field name="model">product.gate.in</field>
            <field name="type">form</field>
            <field name="arch" type="xml">
                <form string="Product Get In Form">
                    <header>
                        <button string="Confirm" name="action_confirm" states="draft" type="object"
                                class="oe_highlight"/>
                        <!--<button name="action_draft" type="object"-->
                                <!--string="Reset to Draft"-->
                                <!--states="confirm" groups="base_technical_features.group_technical_features"/>-->
                        <field name="state" widget="statusbar" statusbar_visible="draft,confirm"/>
                    </header>
                    <sheet>
                        <group>
                            <div class="oe_button_box" name="button_box">
                                <button class="oe_stat_button" name="action_get_stock_picking"
                                        string="Stock Operation" type="object"
                                        attrs="{'invisible':[('picking_id','=',False)]}" icon="fa-cogs"
                                        groups="stock.group_stock_user"/>
                                <field name="picking_id" invisible="1"/>
                            </div>
                            <group>
                                <field name="receive_type" placeholder="Please Select"/>
                                <field name="date"/>
                                <field name="challan_bill_no"/>
                                <field name="truck_no"/>
                            </group>
                            <group>
                                <field name="ship_id"
                                       options="{'no_quick_create':True,'no_create_edit':True,'no_open': True}"
                                       attrs="{'invisible': [('receive_type', '!=', 'lc')],'required': [('receive_type', '=', 'lc')]}"/>
                                <field name="create_by"/>
                                <field name="received"/>
                                <field name="partner_id" options="{'no_quick_create':True,'no_create_edit':True,'no_open': True}"/>
                            </group>
                        </group>
                        <notebook colspan="4">
                            <page string="Products">
                                <field name="shipping_line_ids">
                                    <tree string="Product Lines" editable="bottom">
                                        <field name="product_id"/>
                                        <field name="product_qty"/>
                                        <field name="product_uom"/>
                                        <field name="price_unit"/>
                                        <field name="state" invisible="1"/>
                                    </tree>
                                </field>
                            </page>
                            <page string="Others">
                                <group>
                                    <group>
                                        <field name="company_id" groups="base.group_multi_company"
                                               options="{'no_create': True,'no_open':True}"/>
                                        <field name="operating_unit_id" options="{'no_create': True,'no_open':True}"
                                               groups="operating_unit.group_multi_operating_unit"/>
                                    </group>
                                </group>

                            </page>
                        </notebook>
                    </sheet>

                </form>
            </field>
        </record>

        <record id="view_product_gate_in_search" model="ir.ui.view">
            <field name="name">product.gate.in.search</field>
            <field name="model">product.gate.in</field>
            <field name="arch" type="xml">
                <search string="Search Gate In">
                    <field name="name"/>
                    <field name="lc_id"/>
                </search>
            </field>
        </record>

        <!-- actions opening views on models -->
        <record model="ir.actions.act_window" id="product_gate_in_action_window">
            <field name="name">Product Gate In</field>
            <field name="res_model">product.gate.in</field>
            <field name="view_mode">tree,form</field>
            <field name="search_view_id" ref="view_product_gate_in_search"/>
        </record>

        <!-- actions views for gate in button box -->
        <record model="ir.actions.act_window" id="act_product_gate_in">
            <field name="name">Product Gate In</field>
            <field name="res_model">product.gate.in</field>
            <field name="domain">[('ship_id.id','=',active_id)]</field>
            <field name="view_mode">tree,form</field>
        </record>

        <!-- menu categories -->
        <!--<menuitem name="Product Gate In" id="product_gate_in_menu" parent="stock.menu_stock_root"-->
                  <!--groups="gbs_procure_n_commercial_access.group_approve_gate_in"/>-->
        <!--<menuitem name="Gate In" id="product_gate_in_sub_menu" parent="product_gate_in_menu" sequence="5"-->
                  <!--action="product_gate_in_action_window" groups="gbs_procure_n_commercial_access.group_approve_gate_in"/>-->
        <menuitem name="Product Gate In" id="product_gate_in_menu" parent="stock.menu_stock_root"/>
        <menuitem name="Gate In" id="product_gate_in_sub_menu" parent="product_gate_in_menu" sequence="5"
                  action="product_gate_in_action_window"/>


    </data>

</odoo>