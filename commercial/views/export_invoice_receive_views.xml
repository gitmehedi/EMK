<?xml version="1.0" encoding="UTF-8"?>
<openerp>
	<data>
		<!-- Export Invoice Receive Tree or List view -->
		<record model="ir.ui.view" id="export_invoice_receive_tree_view" >
			<field name="name">export.invoice.receive.tree</field>
			<field name="model">export.invoice.receive</field>
			<field name="arch" type="xml">
				<tree string="Export Invoice Receive Tree">
					<field name="name" />
					<field name="submission_date" />
					<field name="bank_ref_no" />
					<field name="submission_type" />
					<field name="currency" />
					<field name="state" />
				</tree>
			</field>
		</record>
		<!-- Export Invoice Receive Form Views -->
		<record model="ir.ui.view" id="export_invoice_receive_form_view">
			<field name="name">export.invoice.receive.form</field>
			<field name="model">export.invoice.receive</field>
			<field name="arch" type="xml">
				<form string="Export Invoice Receive Form">
					<header>
						<button name="action_confirm" type="object" states="draft"
							string="Confirm" class="oe_highlight" />

						<field name="state" widget="statusbar" />
					</header>
					<sheet>
						<h1>
							<field name="name" string="Serial No" />
						</h1>
						<group>
							<group>
								<field name="submission_date"
									options='{"no_open": True,"no_create": 1, "no_create_edit": 1}' />
								<field name="bank_ref_no" />
								<field name="submission_type"
									options='{"no_open": True,"no_create": 1, "no_create_edit": 1}' />
							</group>
							<group>
								<field name="realization_date" />
								<field name="currency"
									options='{"no_open": True,"no_create": 1, "no_create_edit": 1}' />
							</group>
						</group>
						<notebook colspan="4">
							<page string="Invoice Receive Details">
								<field name="invoice_receive_details_ids">
									<tree string="" editable="bottom">
										<field name="invoice_id"
											options='{"no_open": True,"no_create": 1, "no_create_edit": 1}' />
										<field name="value" />
									</tree>
								</field>
							</page>
							<page string="Negotiation Details">
								<field name="invoice_receive_negotiation_ids">
									<tree string="" editable="bottom">
										<field name="distribution_account_id"
											options='{"no_open": True,"no_create": 1, "no_create_edit": 1}' />
										<field name="fc_value" />
										<field name="lc_value" />
										<field name="exchange_rate" />
									</tree>
								</field>
							</page>
						</notebook>
						<separator string="Remarks" />
						<field name="remarks" placeholder="Put an remarks..................." />
					</sheet>
				</form>
			</field>
		</record>




		<!-- Export Invoice Receive Search View -->
		<record model="ir.ui.view" id="export_invoice_receive_view_search" >
			<field name="name">export.invoice.receive.search</field>
			<field name="model">export.invoice.receive</field>
			<field eval="10" name="priority" />
			<field name="arch" type="xml">
				<search string="Export Invoice Receive">
					<field name="name" />
					<field name="submission_date" />
					<field name="bank_ref_no" />
					<field name="submission_type" />
				</search>
			</field>
		</record>

		<!-- Sequences for Export Invoice Receive -->
		<record model="ir.sequence.type" id="seq_type_export_invoice_receive" >
			<field name="name">Export Invoice Receive Type</field>
			<field name="code">eir_code</field>
		</record>

		<record model="ir.sequence" id="seq_export_invoice_receive" >
			<field name="name">Export Invoice Receive</field>
			<field name="code">eir_code</field>
			<field name="prefix">EIR-</field>
			<field name="padding">10</field>
		</record>


		<!-- Export Invoice Receive Menu and Action -->
		<record model="ir.actions.act_window" id="export_invoice_receive_list_action" >
			<field name="name">Export Invoice Receive</field>
			<field name="res_model">export.invoice.receive</field>
			<field name="view_type">form</field>
			<field name="view_mode">tree,form</field>
		</record>

		<menuitem id="export_invoice_receive_main_menu" name="Export Invoice Receive"
			parent="commercial_menu" sequence="125"
			action="export_invoice_receive_list_action" />
	</data>
</openerp>
