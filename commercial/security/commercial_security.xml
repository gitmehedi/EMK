<?xml version="1.0" encoding="utf-8"?>
<odoo>
<data noupdate="0">

    <record id="module_category_commercial" model="ir.module.category" >
        <field name="name">Commercial</field>
        <field name="sequence">210</field>
    </record>

    <record id="group_commercial_user" model="res.groups">
        <field name="name">User: Import</field>
        <field name="category_id" ref="module_category_commercial"/>
        <field name="sequence">40</field>
    </record>

    <record id="group_commercial_manager" model="res.groups">
        <field name="name">Manager: Import</field>
        <field name="category_id" ref="module_category_commercial"/>
        <field name="implied_ids" eval="[(4, ref('group_commercial_user'))]"/>
        <field name="sequence">20</field>
    </record>

    <record id="group_commercial_user_export" model="res.groups">
        <field name="name">User: Export</field>
        <field name="category_id" ref="module_category_commercial"/>
        <field name="sequence">30</field>
    </record>

    <record id="group_commercial_manager_export" model="res.groups">
        <field name="name">Manager: Export</field>
        <field name="category_id" ref="module_category_commercial"/>
        <field name="implied_ids" eval="[(4, ref('group_commercial_user_export'))]"/>
        <field name="sequence">10</field>
    </record>

</data>
</odoo>
