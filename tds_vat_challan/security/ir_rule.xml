<?xml version="1.0" encoding="utf-8" ?>
<odoo>
    <data>
        <record id="tds_challan_rule" model="ir.rule">
            <field name="name">TDS Challan Rule</field>
            <field name="model_id" ref="tds_vat_challan.model_tds_vat_challan"/>
            <field name="domain_force">[('operating_unit_id.id','in',user.operating_unit_ids.ids)]</field>
            <field name="perm_read" eval="True" />
            <field name="perm_create" eval="True"/>
            <field name="perm_write" eval="True"/>
            <field name="perm_unlink" eval="True"/>
            <field name="groups" eval="[(4,ref('account.group_account_manager')),(4,ref('account.group_account_user'))]"/>
        </record>
    </data>
</odoo>