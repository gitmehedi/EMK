<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data>
        <record id="view_stock_picking_type_tree_inherit" model="ir.ui.view">
            <field name="name">view.stock.picking.type.tree.inherit</field>
            <field name="model">stock.picking.type</field>
            <field name="inherit_id" ref="stock.view_picking_type_tree" />
            <field name="arch" type="xml">
                <tree>

                </tree>
            </field>
        </record>
        <record id="view_stock_picking_type_form_inherit" model="ir.ui.view">
            <field name="name">view.stock.picking.type.form.inherit</field>
            <field name="model">stock.picking.type</field>
            <field name="inherit_id" ref="stock.view_picking_type_form" />
            <field name="arch" type="xml">
                <xpath expr="//field[@name='name']" position="after">
                  <field name="state" invisible="1"/>
                </xpath>
            </field>
        </record>

        <record id="action_stock_picking_type" model="ir.actions.act_window">
            <field name="name">Stock Picking Type</field>
            <field name="res_model">stock.picking.type</field>
            <field name="view_mode">tree,form</field>
        </record>

        <menuitem id="menu_stock_picking_type" name="Stock Picking Type"
                  parent="stock.menu_stock_config_settings"
                  action="action_stock_picking_type" sequence="37" />
    </data>

</odoo>