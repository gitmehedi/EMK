<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data>
        <record id="view_stock_product_product_tree_inherit" model="ir.ui.view">
            <field name="name">view.stock.product.product.tree.inherit</field>
            <field name="model">product.product</field>
            <field name="inherit_id" ref="gbs_stock_product.product_product_tree_view1" />
            <field name="arch" type="xml">
                <tree>

                </tree>
            </field>
        </record>
        <record id="view_stock_product_product_form_inherit" model="ir.ui.view">
            <field name="name">view.stock.product.product.form.inherit</field>
            <field name="model">product.product</field>
            <field name="inherit_id" ref="product.product_normal_form_view" />
            <field name="arch" type="xml">
                <xpath expr="//field[@name='name']" position="after">
                  <field name="state" invisible="1"/>
                </xpath>
            </field>
        </record>

        <record model="ir.actions.act_window" id="action_product_setup_tree">
           <field name="name">Product Setup</field>
            <field name="res_model">product.product</field>
            <field name="view_mode">tree,form</field>
            <field name="domain">[('type','=','product')]</field>
             <field name="context">
               {"search_default_active":'False',"search_default_pending":'True',"search_default_type":1}
            </field>
        </record>

        <menuitem id="purchase_product_setup" name="Product" parent="stock_indent_menu.menu_stock_purchase"
                  sequence="25" action="action_product_setup_tree"/>
    </data>
</odoo>
