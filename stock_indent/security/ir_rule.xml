<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <record id="rule__multi_stock_location" model="ir.rule">
        <field name="name">Rule Multi Stock Location(User,Manager)</field>
        <field name="model_id" ref="stock.model_stock_location"/>
        <field name="domain_force">[('id','in',user.location_ids.ids)]</field>
        <field name="perm_read" eval="True" />
        <field name="perm_create" eval="True"/>
        <field name="perm_write" eval="True"/>
        <field name="perm_unlink" eval="True"/>
        <field name="groups" eval="[(4,ref('stock.group_stock_user')),(4,ref('stock.group_stock_manager'))]"/>
    </record>
</odoo>

