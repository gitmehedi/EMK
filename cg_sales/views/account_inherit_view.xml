<?xml version="1.0" encoding="utf-8"?>
<openerp>
    <data>
    
    <record id="view_account_voucher_form" model="ir.ui.view">
            <field name="name">account.voucher.form.inherit</field>
            <field name="model">account.voucher</field>
            <field name="priority">3</field>
            <field name="inherit_id" ref="account_voucher.view_vendor_receipt_form"/>
            <field name="arch" type="xml">
                <xpath expr='//group/group/field[@name="journal_id"]' position="after">
                    <field name="bank_id" options='{"no_open": True, "no_create": True}'/>
                    <field name="branch_name"/>
                    <field name="cheque_no"/>
                </xpath>
                <xpath expr='//group/group/field[@name="date"]' position="before">
                    <field name="receipt_date"/>
                </xpath>
            </field>
        </record>


	
    </data>
</openerp>