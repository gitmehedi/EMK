<?xml version="1.0" encoding="UTF-8"?>

<odoo noupdate="1">

    <record id="ir_cron_commission_refund_monthly_update" model="ir.cron">
        <field name="name">Commission Refund Monthly</field>
        <field name="active">0</field>
        <field name="user_id" ref="base.user_root" />
        <field name="interval_number">1</field>
        <field name="interval_type">days</field>
        <field name="numbercall">-1</field> <!-- (-1) indicates unlimited -->
        <field name="nextcall"
               eval="(datetime.now() + timedelta(minutes=1)).strftime('%Y-%m-%d 01:00:00')"/>
        <field name="model">commission.refund.prcntg.config</field>
        <field name="function">pull_automation_monthly</field>
    </record>
    <record id="ir_cron_commission_refund_yearly_update" model="ir.cron">
        <field name="name">Commission Refund Yearly</field>
        <field name="active">0</field>
        <field name="user_id" ref="base.user_root" />
        <field name="interval_number">1</field>
        <field name="interval_type">days</field>
        <field name="numbercall">-1</field> <!-- (-1) indicates unlimited -->
        <field name="nextcall"
               eval="(datetime.now() + timedelta(minutes=1)).strftime('%Y-%m-%d 01:30:00')"/>
        <field name="model">commission.refund.prcntg.config</field>
        <field name="function">pull_automation_yearly</field>
    </record>




</odoo>
