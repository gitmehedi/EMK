<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data>
        <!-- Inherit Form View to Modify it -->
        <record id="res_partner_commission_refund_payable_acc_view_form_" model="ir.ui.view">
            <field name="name">res.partner.commission.refund.account.payable.view.form.2</field>
            <field name="model">res.partner</field>
            <field name="inherit_id" ref="account.view_partner_property_form"/>
            <field name="arch" type="xml">
                <xpath expr="//group[@name='accounting_entries']" position="after">
                    <group></group>
                    <group string="Commission/Refund" name="commission">
                        <field name="commission_refund_account_payable_id"/>
                    </group>
                </xpath>

            </field>
        </record>
    </data>
</odoo>