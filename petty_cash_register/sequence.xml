<?xml version="1.0" encoding="utf-8"?>
<openerp>
    <data noupdate="1">
        
        <!-- Sequences for iou.slip -->
        
        <record id="seq_type_iou_slip" model="ir.sequence.type">
            <field name="name">IOU Slip No</field>
            <field name="code">iou.slip</field>
        </record>

        <record id="seq_iou_slip" model="ir.sequence">
            <field name="name">IOU Slip No</field>
            <field name="code">iou.slip</field>
            <field name="prefix">IS</field>
            <field name="padding">3</field>
            <field name="company_id" eval="False"/>
        </record>
        
        
        <!-- Sequences for iou.adjustment -->
        
        <record id="seq_type_iou_adjustment" model="ir.sequence.type">
            <field name="name">IOU Adjustment No</field>
            <field name="code">iou.adjustment</field>
        </record>

        <record id="seq_iou_adjustment" model="ir.sequence">
            <field name="name">IOU Adjustment No</field>
            <field name="code">iou.adjustment</field>
            <field name="prefix">IA</field>
            <field name="padding">3</field>
            <field name="company_id" eval="False"/>
        </record>

    </data>
</openerp>
