<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data noupdate="1">
        <record id="seq_card_replacement" model="ir.sequence">
            <field name="name">Card Replacement Sequence</field>
            <field name="code">member.card.replacement.seq</field>
