<?xml version='1.0' encoding='UTF-8'?>
<odoo>
    <data noupdate="1">

        <record id="vendor_advance_journal" model="account.journal">
            <field name="name">Vendor Advance</field>
            <field name="type">purchase</field>
            <field name="code">VA</field>
        </record>

        <!-- Sequence Records -->
        <record id="seq_agreement" model="ir.sequence">
            <field name="name">Vendor Agreement</field>
            <field name="code">agreement</field>
            <field name="prefix">VA/%(range_year)s/</field>
            <field eval="1" name="number_next"/>
            <field eval="1" name="number_increment"/>
            <field name="padding">5</field>
        </record>

        <record id="seq_rent_agreement" model="ir.sequence">
            <field name="name">Rent Agreement</field>
            <field name="code">rent.agreement</field>
            <field name="prefix">RA/%(range_year)s/</field>
            <field eval="1" name="number_next"/>
            <field eval="1" name="number_increment"/>
            <field name="padding">5</field>
        </record>

        <record id="payment_instruction_seq" model="ir.sequence">
            <field name="name">Payment Instruction Sequence</field>
            <field name="code">payment.instruction.sequence</field>
            <field name="prefix">PIS</field>
            <field name="padding">12</field>
            <field name="number_increment" eval="1"/>
            <field name="company_id" eval="False"/>
        </record>

        <record id="seq_agreement" model="ir.sequence">
            <field name="name">agreement</field>
            <field name="code">name</field>
            <field name="prefix">VA-</field>
            <field name="padding">5</field>
        </record>
        <record id="seq_vendor_bill_generation" model="ir.sequence">
            <field name="name">Vendor Bill Generation</field>
            <field name="code">vendor.bill.generation</field>
			<field name="active">TRUE</field>
            <field name="prefix">VBG-</field>
            <field name="padding">5</field>
			<field name="number_increment">1</field>
        </record>
        <record id="seq_receive_outstanding_advance" model="ir.sequence">
            <field name="name">Receive Outstanding Advance</field>
            <field name="code">receive.outstanding.advance</field>
            <field name="prefix">ROA/%(range_year)s/</field>
            <field eval="1" name="number_next"/>
            <field eval="1" name="number_increment"/>
            <field name="padding">5</field>
        </record>

        <record id="seq_vendor_security_deposit" model="ir.sequence">
            <field name="name">Vendor Security Deposit</field>
            <field name="code">vendor.security.deposit</field>
            <field name="prefix">VSD/%(range_year)s/</field>
            <field eval="1" name="number_next"/>
            <field eval="1" name="number_increment"/>
            <field name="padding">5</field>
        </record>

        <act_window id="action_rent_agreement_wizard"
                    name="Rent Agreement"
                    src_model="agreement"
                    res_model="rent.payment.instruction.wizard"
                    view_type="form"
                    view_mode="form"
                    key2="client_action_multi"
                    target="new"/>
    </data>
</odoo>