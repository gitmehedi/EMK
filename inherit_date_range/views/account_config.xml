<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <record id="view_mdmc_bulk_upload_account_config" model="ir.ui.view">
        <field name="name">view.mdmc.bulk.config</field>
        <field name="model">account.config.settings</field>
        <field name="inherit_id" ref="account.view_account_config_settings"/>
        <field name="arch" type="xml">
            <xpath expr="//group[@name='accounting']" position="after">
                <group string="General/Banking" name="grp_tds_vat_challan">
                    <label for="id" string="EOY Type" groups="account.group_account_user"/>
                    <div name="tds_vat_challan">
                        <div>
                            <field name="eoy_type" class="oe_inline"/>
                        </div>
                    </div>
                    <label for="id" string="Retain Earnings Account" groups="account.group_account_user"/>
                    <div name="tds_vat_challan">
                        <div>
                            <field name="retain_earning_id" class="oe_inline"
                                   options="{'no_create':True,'no_create_edit':True,'no_open':True}"/>
                        </div>
                    </div>
                    <label for="id" string="General Account" groups="account.group_account_user"/>
                    <div name="tds_vat_challan">
                        <div>
                            <field name="general_account_id" class="oe_inline"
                                   options="{'no_create':True,'no_create_edit':True,'no_open':True}"/>
                        </div>
                    </div>
                </group>
            </xpath>
        </field>
    </record>
</odoo>