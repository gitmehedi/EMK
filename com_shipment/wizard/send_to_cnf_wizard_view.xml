<?xml version="1.0" encoding="utf-8" ?>
<odoo>
    <data>
        <record id="send_to_cnf_wizard" model="ir.ui.view">
            <field name="name">send.to.cnf.wizard</field>
            <field name="model">send.to.cnf.wizard</field>
            <field name="type">form</field>
            <field name="arch" type="xml" >
                <form string="Please Enter The Information" create="false" edit="false">
                    <group>
                        <group>
                            <field name="cnf_received_date"/>
                            <field name="cnf_id" string="C&amp;F Agent"
                            options="{'no_open':True,'no_quick_edit':True,'no_create_edit':True}"/>
                        </group>
                    </group>
                    <footer>
                        <button string="Save" name="save_send_to_cnf" type="object" class="oe_highlight"  />
                        <button special="cancel" string="Close" type="object"/>
                    </footer>
                </form>
            </field>
        </record>
    </data>
</odoo>