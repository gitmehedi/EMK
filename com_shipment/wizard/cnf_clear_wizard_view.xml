<?xml version="1.0" encoding="utf-8" ?>
<odoo>
    <data>
        <record id="cnf_clear_wizard" model="ir.ui.view">
            <field name="name">cnf.clear.wizard</field>
            <field name="model">cnf.clear.wizard</field>
            <field name="type">form</field>
            <field name="arch" type="xml" >
                <form string="Please Enter The Information" create="false" edit="false">
                    <group>
                        <group>
                            <field name="arrival_date"/>
                        </group>
                        <group>
                            <field name="transport_by"/>
                            <field name="vehical_no"/>
                        </group>
                    </group>
                    <footer>
                        <button string="Save" name="save_cnf_clear" type="object" class="oe_highlight"  />
                        <button special="cancel" string="Close" type="object"/>
                    </footer>
                </form>
            </field>
        </record>
    </data>
</odoo>