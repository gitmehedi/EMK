<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <record id="inherit_invoice_supplier_form_sd" model="ir.ui.view">
        <field name="name">inherit.account.invoice.form.sd</field>
        <field name="model">account.invoice</field>
        <field name="inherit_id" ref="account.invoice_supplier_form"/>
        <field name="arch" type="xml">
            <xpath expr="//div[@class='oe_inline']" position="before">
                <group>
                    <field name="security_deposit" class="oe_subtotal_footer_separator"/>
                </group>
            </xpath>
        </field>
    </record>
</odoo>