<?xml version='1.0' encoding='UTF-8'?>
<odoo>
    <data noupdate="1">

        <record id="seq_vendor_security_return" model="ir.sequence">
            <field name="name">Vendor Security Return</field>
            <field name="code">vendor.security.return</field>
			<field name="active">TRUE</field>
            <field name="prefix">VSR/%(range_year)s/</field>
            <field name="padding">5</field>
			<field name="number_increment">1</field>
            <field name="implementation">no_gap</field>
        </record>
        <record id="seq_vendor_security_deposit" model="ir.sequence">
            <field name="name">Vendor Security Return</field>
            <field name="code">vendor.security.deposit</field>
			<field name="active">TRUE</field>
            <field name="prefix">VSD/%(range_year)s/</field>
            <field name="padding">5</field>
			<field name="number_increment">1</field>
            <field name="implementation">no_gap</field>
        </record>

    </data>
</odoo>