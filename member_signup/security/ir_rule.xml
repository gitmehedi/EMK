<?xml version="1.0" encoding='utf-8'?>
<odoo>
    <record model="ir.rule" id="rule_mem_app">
        <field name="name">Applicant</field>
        <field name="model_id" ref="model_res_partner"></field>
        <field name="domain_force">[('id','=',user.partner_id.id)]</field>
        <field name="global" eval="True"/>
        <field eval="1" name="perm_read"></field>
        <field eval="1" name="perm_write"></field>
        <field eval="0" name="perm_unlink"></field>
        <field eval="0" name="perm_create"></field>
    </record>

    <record model="ir.rule" id="rule_mem_member">
        <field name="name">Rule of Member</field>
        <field name="model_id" ref="model_res_partner"></field>
        <field name="groups" eval="[(4,ref('membership_user.group_user_member'))]"></field>
        <field name="domain_force">[('id','=',user.partner_id.id)]</field>
        <field eval="1" name="perm_read"></field>
        <field eval="1" name="perm_write"></field>
        <field eval="0" name="perm_unlink"></field>
        <field eval="0" name="perm_create"></field>
    </record>

    <record model="ir.rule" id="rule_mem_officer">
        <field name="name">Rule of Membership Officer</field>
        <field name="model_id" ref="model_res_partner"></field>
        <field name="groups" eval="[(4,ref('membership_user.group_mem_user_officer'))]"></field>
        <field eval="1" name="perm_read"></field>
        <field eval="1" name="perm_write"></field>
        <field eval="0" name="perm_unlink"></field>
        <field eval="1" name="perm_create"></field>
    </record>

</odoo>