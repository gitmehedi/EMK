<?xml version="1.0" encoding="UTF-8"?>

<odoo noupdate="1">
    <record id="ir_cron_server_file_process_scheduler" model="ir.cron">
        <field name="name">File Processing Scheduler</field>
        <field name="user_id" ref="base.user_root"/>
        <field name="interval_number">1</field>
        <field name="interval_type">days</field>
        <field name="numbercall">-1</field>
        <field name="nextcall"
               eval="(datetime.now() + timedelta(days=1)).strftime('%Y-%m-%d 02:00:00')"/>
        <field name="model">server.file.process</field>
        <field name="function">action_backup_all</field>
    </record>
    <record id="ir_cron_soap_process_scheduler" model="ir.cron">
        <field name="name">Generic Transfer</field>
        <field name="user_id" ref="base.user_root"/>
        <field name="interval_number">1</field>
        <field name="interval_type">days</field>
        <field name="numbercall">-1</field>
        <field name="nextcall" eval="(datetime.now() + timedelta(days=1)).strftime('%Y-%m-%d 02:00:00')"/>
        <field name="model">soap.process</field>
        <field name="function">soap_request</field>
    </record>
</odoo>
